{
    "require": {
        "symfony/console": "^3.3",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5",
        "phpunit/php-code-coverage": "^5.3",
        "composer/xdebug-handler": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Console\\": "src"
        },
        "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
